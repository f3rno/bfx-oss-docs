{% extends 'layouts/bfx_base.njk' %}

{% block page_title %}
{{ data.title }}
{% endblock %}

{% block sidebar %}
<div
  class="bfx-oss-docs-side-bar__sub-menu"
  id="bfx-oss-docs-side-bar__api-reference-menu"
>
  <h6>{{ sidebarMenuTitle }}</h6>
  <ul>
    {% for item in sidebarMenuData %}
    <li
      {% if item.href === fn %}
      class="active"
      {% endif %}
    >
      <a href="/{{ item.href }}">{{ item.label }}</a>
    </li>
    {% endfor %}
  </ul>
</div>
{% endblock %}

{% block content %}
<div
  id="bfx-oss-docs-content-wrapper"

  {% if contentType %}
  class="bfx-oss-docs-content-type-{{ contentType }}"
  {% endif %}
>
  <div class="bfx-oss-docs-md-content bfx-oss-docs-code-wrapper">
    {{ html | safe }}
  </div>
</div>
{% endblock %}
