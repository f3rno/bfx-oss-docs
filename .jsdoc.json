{
  "tags": {
    "allowUnknownTags": false,
    "dictionaries": ["jsdoc"]
  },
  "source": {
    "include": ["./scripts", "./lib", "./docs/pages/index.md"],
    "includePattern": ".js$",
    "excludePattern": "(node_modules|docs)"
  },
  "plugins": [
    "plugins/markdown"
  ],
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": true,
    "smallSourceLink": true,
    "hideAuthor": true
  },
  "opts": {
    "destination": "./docs/dist/",
    "encoding": "utf8",
    "private": false,
    "recurse": true,
    "template": "./node_modules/docdash"
  },
  "docdash": {
    "static": true,
    "sort": true,
    "search": true
  }
}
