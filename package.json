{
  "name": "bfx-oss-docs",
  "version": "1.0.0",
  "description": "Official Bitfinex Documentation",
  "main": "lib/index.js",
  "engines": {
    "node": ">=14.2.0"
  },
  "homepage": "https://github.com/bitfinexcom/bfx-oss-docs#readme",
  "license": "MIT",
  "contributors": [
    "Cris Mihalache <cris.m@bitfinex.com> (https://www.bitfinex.com)",
    "Paolo Ardoino <paolo@bitfinex.com> (https://www.bitfinex.com)",
    "Jacob Plaster <jacob.plaster@bitfinex.com> (https://www.bitfinex.com)"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bitfinexcom/bfx-oss-docs.git"
  },
  "bugs": {
    "url": "https://github.com/bitfinexcom/bfx-oss-docs/issues"
  },
  "keywords": [
    "bitfinex",
    "jsdoc",
    "library",
    "trading",
    "indicators",
    "automation",
    "framework"
  ],
  "scripts": {
    "lint": "./node_modules/.bin/eslint --ext .js ./lib/index.js ./lib/js ./test/",
    "unit": "NODE_ENV=test ./node_modules/.bin/nyc ./node_modules/.bin/mocha --recursive",
    "test": "yarn lint && yarn unit",
    "clean": "yarn clean-jsdoc && yarn clean-site",
    "clean-jsdoc": "rm -rf ./dist/jsdoc/* && touch ./dist/jsdoc/.keep",
    "clean-site": "rm -rf ./dist/site/* && touch ./dist/site/.keep",
    "prepare": "yarn prepare-submodules && yarn prepare-bfx-hf-chart",
    "prepare-submodules": "git submodule init && git submodule update",
    "prepare-bfx-hf-chart": "cd _assets/npm/bfx-hf-chart && npm i && npm run cra-build",
    "serve-site": "./node_modules/.bin/http-server ./dist/site",
    "serve-jsdoc": "./node_modules/.bin/http-server ./dist/jsdoc",
    "build-site": "yarn clean-site && node scripts/render.js",
    "build-bfx-jsdoc-assets": "cp ./jsdoc_assets/* ./dist/jsdoc/ -r",
    "build-bfx-jsdoc-config": "node scripts/render_jsdoc_config.js",
    "build-bfx-jsdoc-html": "./node_modules/.bin/jsdoc --pedantic --configure .jsdoc-merged.json --verbose",
    "build-bfx-jsdoc": "yarn build-bfx-jsdoc-config && yarn build-bfx-jsdoc-html && yarn build-bfx-jsdoc-assets",
    "build": "yarn build-bfx-jsdoc && yarn build-site",
    "dist": "yarn clean && yarn prepare-bfx-hf-chart && yarn build",
    "postinstall": "yarn dist"
  },
  "dependencies": {
    "@centerforopenscience/markdown-it-imsize": "^2.0.1",
    "@gerhobbelt/markdown-it-hashtag": "^0.4.0-1",
    "@tomleesm/markdown-it-wikilinks": "^1.1.1",
    "fs-extra": "^9.0.0",
    "highlight.js": "^10.0.3",
    "lodash": "^4.17.15",
    "markdown-it": "^10.0.0",
    "markdown-it-abbr": "^1.0.4",
    "markdown-it-anchor": "^5.3.0",
    "markdown-it-attrs": "^3.0.2",
    "markdown-it-broken-image-placeholder": "^1.1.0",
    "markdown-it-collapsible": "^1.0.0",
    "markdown-it-color": "^1.0.5",
    "markdown-it-container": "^2.0.0",
    "markdown-it-deflist": "^2.0.3",
    "markdown-it-emoji": "^1.4.0",
    "markdown-it-expand-tabs": "^1.0.13",
    "markdown-it-external-links": "^0.0.6",
    "markdown-it-footnote": "^3.0.2",
    "markdown-it-front-matter": "^0.2.1",
    "markdown-it-grouped-code-fence": "^1.2.0",
    "markdown-it-header-sections": "^1.0.0",
    "markdown-it-hierarchy": "^1.1.0",
    "markdown-it-highlightjs": "^3.1.0",
    "markdown-it-iframe": "^1.0.0",
    "markdown-it-implicit-figures": "^0.10.0",
    "markdown-it-inline-comments": "^1.0.1",
    "markdown-it-ins": "^3.0.0",
    "markdown-it-label": "^1.0.0",
    "markdown-it-mark": "^3.0.0",
    "markdown-it-multimd-table": "^4.0.1",
    "markdown-it-plugin-data-src": "^1.0.0",
    "markdown-it-regexp": "^0.4.0",
    "markdown-it-relativelink": "^0.2.0",
    "markdown-it-replacements": "^1.0.1",
    "markdown-it-smartarrows": "^1.0.1",
    "markdown-it-sub": "^1.0.0",
    "markdown-it-sup": "^1.0.0",
    "markdown-it-task-lists": "^2.1.1",
    "markdown-it-toc-done-right": "^4.1.0",
    "nunjucks": "^3.2.1",
    "nunjucks-extension-code-highlight": "^0.0.1",
    "nunjucks-highlight.js": "^0.0.5",
    "nunjucks-markdown": "^2.0.1",
    "sass": "^1.26.5",
    "string": "^3.3.3",
    "stylus": "^0.54.7",
    "uslug": "^1.0.4"
  },
  "devDependencies": {
    "babel-eslint": "^10.1.0",
    "chai": "^4.2.0",
    "concurrently": "^5.2.0",
    "docdash": "^1.2.0",
    "eslint": "^6.8.0",
    "eslint-config-standard": "^14.1.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-jsdoc": "^22.0.0",
    "eslint-plugin-lodash": "^6.0.0",
    "eslint-plugin-mocha": "^6.3.0",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "http-server": "^0.12.1",
    "husky": "^4.2.3",
    "jsdoc": "^3.6.4",
    "jsdoc-mermaid": "^1.0.0",
    "jsdoc-plugin-npm": "^1.0.3",
    "jsdoc-plugin-url": "^1.0.1",
    "jsdoc-to-markdown": "^5.0.3",
    "mocha": "^7.1.0",
    "nyc": "^15.0.0",
    "standard": "^14.1.0"
  }
}
