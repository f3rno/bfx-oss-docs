{
  "output": {
    "staticSite": "./dist/site",
    "unifiedJSDocDist": "./dist/jsdoc",
    "unifiedJSDocConfig": "./.jsdoc-merged.json"
  },

  "layouts": {
    "engine": "nunjucks",
    "includePath": "_includes",
    "pagesPath": "_pages",
    "options": {
      "autoescape": false,
      "throwOnUndefined": true,
      "trimBlocks": true
    }
  },

  "assets": {
    "svg": "_assets/svg",
    "svg/svg-icons": "_assets/npm/svg-icon/dist/svg",
    "css/highlightjs": "_assets/npm/highlight.js/src/styles",
    "bfx-hf-chart": "_assets/npm/bfx-hf-chart/build"
  },

  "styles": {
    "config": {
      "compressed": true,
      "sourceMap": true,
      "sourceMapContents": true,
      "includePaths": [
        "_styles/"
      ]
    },

    "sources": {
      "index.css": "_styles/main.scss"
    }
  },

  "markdown": {
    "parser": "markdown-it",
    "options": {
      "html": true,
      "linkify": true,
      "typographer": true,
      "quotes": "'“”‘’'"
    },

    "plugins": {
      "markdown-it-emoji": {},
      "markdown-it-highlightjs": {
        "auto": false,
        "code": true
      }
    }
  },

