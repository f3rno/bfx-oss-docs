{
  "output": {
    "staticSite": "./dist/site",
    "convertedRIOAPIDocs": "./_src/api",
    "unifiedJSDocDist": "./dist/jsdoc",
    "unifiedJSDocConfig": "./.jsdoc-merged.json"
  },

  "structure": {
    "root": "./_src",
    "sections": [{
      "id": "home",
      "type": "page",
      "root": "./home",
      "label": "Home",
      "filename": "index.html"
    }, {
      "id": "v2_overview",
      "type": "page-group",
      "root": "./api/v2/overview",
      "label": "API v2 Overview",
      "groupIndex": "introduction"
    }, {
      "id": "v2_reference",
      "type": "page-group-inline",
      "root": "./api/v2/reference",
      "label": "API v2 Reference"
    }, {
      "id": "v1_overview",
      "type": "page-group",
      "root": "./api/v1/overview",
      "label": "API v1 Overview",
      "groupIndex": "introduction"
    }, {
      "id": "v1_reference",
      "type": "page-group-inline",
      "root": "./api/v1/reference",
      "label": "API v1 Reference"
    }, {
      "id": "oss_projects",
      "type": "page",
      "root": "./oss_projects",
      "label": "Open Source Projects"
    }]
  },

  "layouts": {
    "engine": "nunjucks",
    "includePath": "_includes",
    "pagesPath": "_pages",
    "options": {
      "autoescape": false,
      "throwOnUndefined": true,
      "trimBlocks": true
    }
  },

  "assets": {
    "img": "_assets/img",
    "fonts": "_assets/fonts",
    "bfx-hf-chart": "node_modules/bfx-hf-chart/build",

    "css/highlightjs": "node_modules/highlight.js/styles",
    "css/normalize.css": "node_modules/normalize.css/normalize.css",
    "js/vendor/zepto.min.js": "node_modules/zepto/dist/zepto.min.js",
    "js/vendor/lodash.min.js": "node_modules/lodash/lodash.min.js",
    "js/vendor/debug.min.js": {
      "path": "node_modules/debug/src/browser.js",
      "build": "browserify",
      "pathOnWindow": "debug",
      "baseDir": "node_modules/debug/src"
    }
  },

  "scripts": {
    "babelConfig": {
      "sourceMaps": "inline",
      "sourceType": "script",
      "compact": true,
      "minified": true,
      "comments": false,
      "presets": [
        ["@babel/preset-env", {
          "targets": "> 0.25%, not dead"
        }]
      ],

      "plugins": [
        "@babel/plugin-proposal-object-rest-spread"
      ]
    }
  },

  "styles": {
    "config": {
      "compressed": true,
      "sourceMap": true,
      "sourceMapContents": true,
      "includePaths": [
        "_styles/"
      ]
    },

    "sources": {
      "index.css": "_styles/main.scss"
    }
  },

  "markdown": {
    "parser": "markdown-it",
    "options": {
      "html": true,
      "linkify": true,
      "typographer": true
    },

    "plugins": {
      "markdown-it-emoji": {},
      "markdown-it-highlightjs": {
        "auto": true,
        "code": true
      }
    }
  },

