{
  "output": {
    "static_site": "./dist/site",
    "jsdoc_dist": "./dist/jsdoc",
    "jsdoc_conf": ".jsdoc-merged.json"
  },

  "routing": {
    "/": "index.njk",
    "/libraries/$repo": "libraries/$repo.njk"
  },

  "layouts": {
    "engine": "nunjucks",
    "include_path": "_includes",
    "pages_path": "_pages",
    "options": {
      "autoescape": false,
      "throwOnUndefined": true,
      "trimBlocks": true
    }
  },

  "assets": {
    "svg": "_assets/svg",
    "svg/svg-icons": "_assets/npm/svg-icon/dist/svg",
    "css/highlightjs": "_assets/npm/highlight.js/src/styles",
    "bfx-hf-chart": "_assets/npm/bfx-hf-chart/build"
  },

  "styles": {
    "config": {
      "compressed": true,
      "sourceMap": true,
      "sourceMapContents": true,
      "includePaths": [
        "_styles/"
      ]
    },

    "sources": {
      "index.css": "_styles/main.scss"
    }
  },

  "markdown": {
    "parser": "markdown-it",
    "options": {
      "html": true,
      "linkify": true,
      "typographer": true,
      "quotes": "'“”‘’'"
    },

    "plugins": {
      "markdown-it-emoji": {},
      "markdown-it-highlightjs": {
        "auto": false,
        "code": true
      }
    }
  },

  "package-root": "./modules",

  "readmeio-data": {
    "root": "./data/readmeio",
